{
  "name": "anime-omikuji",
  "version": "0.0.0",
  "type": "module",
  "author": "SlashNephy <spica@starry.blue>",
  "repository": {
    "url": "https://github.com/SlashNephy/anime-omikuji",
    "type": "git"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "graphql-codegen --config graphql.codegen.ts",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "6.0.7",
    "@mantine/hooks": "6.0.7",
    "@nextui-org/react": "1.0.0-beta.12",
    "@tabler/icons-react": "2.16.0",
    "@tanstack/react-query": "4.29.3",
    "country-flag-icons": "1.5.7",
    "date-fns": "2.29.3",
    "font-color-contrast": "11.1.0",
    "graphql-request": "5.2.0",
    "lz-string": "1.5.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.7",
    "recoil-sync": "0.2.0",
    "ts-array-length": "0.1.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@slashnephy/eslint-config": "0.4.18",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "concurrently": "8.0.1",
    "eslint": "8.39.0",
    "graphql": "16.6.0",
    "prettier": "2.8.8",
    "typescript": "4.9.5",
    "vite": "4.3.2"
  },
  "packageManager": "yarn@3.5.0",
  "engines": {
    "node": ">= 18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": "graphql.codegen.ts",
        "parserOptions": {
          "project": "./tsconfig.node.json"
        },
        "rules": {
          "import/no-default-export": "off"
        }
      }
    ]
  }
}
