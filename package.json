{
  "name": "anime-omikuji",
  "version": "0.0.0",
  "type": "module",
  "author": "SlashNephy <spica@starry.blue>",
  "repository": "https://github.com/SlashNephy/anime-omikuji",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "graphql-codegen --config graphql/codegen.yml",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.12",
    "@tabler/icons-react": "2.10.0",
    "@tanstack/react-query": "4.24.10",
    "country-flag-icons": "1.5.5",
    "date-fns": "2.29.3",
    "graphql-request": "5.1.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.7",
    "ts-array-length": "0.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/import-types-preset": "2.2.6",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-graphql-request": "4.5.8",
    "@graphql-codegen/typescript-operations": "3.0.2",
    "@slashnephy/eslint-config": "0.3.10",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.2.0",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "graphql": "16.6.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.2.0"
  },
  "packageManager": "yarn@3.4.1",
  "engines": {
    "node": ">= 18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
